{
  "name": "medical-chatbot",
  "version": "1.0.0",
  "description": "Medical AI Chatbot for Hospital Website",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "start": "node dist/server/index.js",
    "dev:server": "cross-env NODE_ENV=development node --loader ts-node/esm src/server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1",
    "vite": "^5.1.4"
  }
